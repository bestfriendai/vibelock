{
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true
      },
      "android": {
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "m-medium",
        "buildType": "aab"
      }
    },
    "production-simulator": {
      "extends": "production",
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "trappat@gmail.com"
      },
      "android": {
        // Upload your service account key using: eas secret:create --scope project --name GOOGLE_SERVICE_ACCOUNT_KEY --type file --value ./path/to/your-service-account-key.json
        "serviceAccountKeyPath": "GOOGLE_SERVICE_ACCOUNT_KEY",
        "track": "production"
      }
    }
  }
}
